{
  "baseurl":"https://rmdhnreza.my.id",
  "languageCode":"in",
  "paginate":10,
  "title":"rmdhnreza.my.id",
  "DefaultContentLanguage":"en",
  "enableGitInfo":true,
  "enableRobotsTXT":true,
  "canonifyURLs":true,
  "enableEmoji":true,
  "RemovePathAccents":true,
  "pygmentsUseClasses":true,
  "timeZone":"Asia/Jakarta",
  "timeOut":"3600s",
  "module": {
    "imports": [
       {
          "path": "github.com/CaiJimmy/hugo-theme-stack/v3",
          "disable":false
       }
    ]
  },
  "frontmatter":{
    "date":[
      "date",
      "lastmod"
    ],
    "lastmod":[
      "lastmod",
      ":git",
      "date",
      "publishDate"
    ]
  },
  "permalinks":{
    "post":"/:title"
  },
  "params":{
    "favicon":"/favicon.svg",
    "mainSections":[
      "post"
    ],
    "featuredImageField":"image",
    "rssFullContent":true,
    "footer":{
      "since":2020
    },
    "dateFormat":{
      "published":"Jan 02, 2006",
      "lastUpdated":"Jan 02, 2006 15:04 UTC"
    },
    "sidebar":{
      "compact":true,
      "subtitle":"Blog Tutorial For Linux & Windows",
      "avatar":{
        "enabled":false,
        "local":true,
        "src":"img/avatar.png"
      }
    },
    "article":{
      "math":false,
      "toc":true,
      "license":{
        "enabled":false,
        "default":"Creative Commons Attribution 4.0 International"
      }
    },
    "comments":{
      "enabled":true,
      "provider":"disqus"
    },
    "widgets":{
      "homepage":[
        {
          "type":"search"
        },
        {
          "type":"archives",
          "params":{
            "limit":5
          }
        },
        {
          "type":"tag-cloud",
          "params":{
            "limit":10
          }
        }
      ],
      "page":[
        {
          "type":"toc"
        }
      ]
    },
    "opengraph":{
      "twitter":{
        "card":"summary_large_image"
      }
    },
    "defaultImage":{
      "opengraph":{
        "enabled":false,
        "local":false
      }
    },
    "colorScheme":{
      "toggle":true,
      "default":"auto"
    },
    "imageProcessing":{
      "cover":{
        "enabled":false
      },
      "content":{
        "enabled":true
      }
    }
  },
  "imaging": {
    "exif": {
      "hint": "photo",
      "quality": 80,
      "resampleFilter": "Lanczos",
      "disableDate": true,
      "disableLatLong": true,
      "excludeFields": ".*",
      "includeFields": ""
    }
 },
  "menu":{
    "main":[
      {
        "identifier":"home",
        "name":"Home",
        "url":"/",
        "weight":-100,
        "params":{
          "icon":"home",
          "newTab":false
        }
      },
      {
        "identifier":"archives",
        "name":"Archives",
        "url":"https://rmdhnreza.my.id/archives",
        "weight":-80,
        "params":{
          "icon":"archives",
          "newTab":false
        }
      },
      {
        "identifier":"source",
        "name":"Source Code",
        "url":"https://github.com/rmdhnreza/rmdhnreza.my.id",
        "weight":-70,
        "params":{
          "icon":"code",
          "newTab":true
        }
      },
      {
        "identifier":"search",
        "name":"Search",
        "url":"https://rmdhnreza.my.id/search",
        "weight":-60,
        "params":{
          "icon":"search",
          "newTab":false
        }
      }
    ]
  },
  "related":{
    "includeNewer":true,
    "threshold":60,
    "toLower":false,
    "indices":[
      {
        "name":"tags",
        "weight":"100"
      },
      {
        "name":"categories",
        "weight":"200"
      }
    ]
  },
  "markup":{
    "defaultMarkdownHandler":"goldmark",
    "goldmark":{
      "renderer":{
        "unsafe":true
      },
      "extensions":{
        "linkify":true,
        "footnote":true
      }
    },
    "highlight":{
      "noClasses":true,
      "anchorLineNos":false,
      "codeFences":true,
      "guessSyntax":false,
      "hl_Lines":"",
      "hl_inline":false,
      "lineAnchors":"",
      "lineNoStart":1,
      "lineNos":true,
      "lineNumbersInTable":true,
      "noHl":true,
      "tabWidth":2
    },
    "tableOfContents":{
      "endLevel":4,
      "ordered":true,
      "startLevel":2
    }
  },
  "build":{
    "writeStats":true
  },
  "outputFormats":{
    "sitemap":{
      "basename":"sitemap",
      "isPlainText":false,
      "mediaType":"application/xml",
      "protocol":"xml://",
      "path":"/"
    }
  },
  "caches":{
    "assets":{
      "dir":":resourceDir/_gen",
      "maxAge":-1
    },
    "getcsv":{
      "dir":":cacheDir/:project",
      "maxAge":-1
    },
    "getjson":{
      "dir":":cacheDir/:project",
      "maxAge":-1
    },
    "getresource":{
      "dir":":resourceDir/_gen",
      "maxAge":-1
    },
    "images":{
      "dir":":resourceDir/_gen",
      "maxAge":-1
    },
    "modules":{
      "dir":":cacheDir/modules",
      "maxAge":-1
    }
  }
}